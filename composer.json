{
  "name": "lukepolo/laracart",
  "description": "A simple cart for Laravel",
  "keywords": [
    "laravel",
    "cart",
    "shopping",
    "shopping cart"
  ],
  "homepage": "https://github.com/lukepolo/laracart",
  "license": "MIT",
  "authors": [
    {
      "name": "Luke Policinski",
      "email": "Luke@LukePOLO.com",
      "homepage": "http://LukePOLO.com"
    }
  ],
  "require": {
    "php": "~7.2 || ~7.3",
    "illuminate/support": "~5.5.0|~5.7.0|~5.8.0|^6.0|^7.0",
    "illuminate/session": "~5.5.0|~5.7.0|~5.8.0|^6.0|^7.0",
    "illuminate/events": "~5.5.0|~5.7.0|~5.8.0|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "orchestra/testbench": "~3.5.0|~3.7.0|~3.8.0",
    "codeclimate/php-test-reporter": "dev-master",
    "mockery/mockery": "^1.0",
    "orchestra/database": "~3.5.0|~3.7.0|~3.8.0"
  },
  "autoload": {
    "psr-4": {
      "LukePOLO\\LaraCart\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LukePOLO\\LaraCart\\Tests\\": "tests"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "LukePOLO\\LaraCart\\LaraCartServiceProvider"
          ],
          "aliases": {
              "LaraCart": "LukePOLO\\LaraCart\\Facades\\LaraCart"
          }
      }
  },
  "minimum-stability": "stable"
}
